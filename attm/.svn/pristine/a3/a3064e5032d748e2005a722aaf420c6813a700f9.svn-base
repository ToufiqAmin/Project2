    <script type="text/javascript">
      
   
   function codeAddress()
	{
	
	$.getJSON("/bgusercontroller/organization/branch", {
	                id : $('#organization').val(),   
	                ajax : 'true'
	            }, function(data) {
	                var html = '<option value=" ">Select Branch</option>';
	                var len = data.length;
	                
	                for ( var i = 0; i < len; i++) {
	                    html += '<option value="' + data[i].orgBranchId + '">'
	                            + data[i].branchName + '</option>';
	                }
	                html += '</option>';
	                $('#branch').html(html);
	            });
	
	}
	
	

	window.onload=codeAddress;
   
    
    $('#organization').change(
	        function() {
	            $.getJSON("/bgusercontroller/organization/branch", {
	                id : $(this).val(),   
	                ajax : 'true'
	            }, function(data) {
	                var html = '<option value=" ">Select Branch</option>';
	                var len = data.length;
	                
	                for ( var i = 0; i < len; i++) {
	                    html += '<option value="' + data[i].orgBranchId + '">'
	                            + data[i].branchName + '</option>';
	                }
	                html += '</option>';
	                $('#branch').html(html);
	            });
	        }); 
	        
	        
	        function showEditModal(index){
			url="/bgusercontroller/phone/"+index;
			
			jQuery.getJSON(url, { }, function (data) {
				document.getElementById("update-phoneId").value=data.phoneId;
				document.getElementById("update-userId").value = data.modelUser.userId;
				document.getElementById("update-typeId").value = data.typeId;
				document.getElementById("updateCountryName").value = data.modelCountry.countryId;
				document.getElementById("update-areaCode").value = data.areaCode;
				document.getElementById("update-phoneNumber").value = data.phoneNumber;
				if(data.activeStatus==1)
				{
					document.getElementById("updateActive").checked=true;
				}
				else 
				{
					document.getElementById("updateActive").checked=false;
				}
				
				if(data.emergencyStatus==1)
				{
					document.getElementById("updateEmergency").checked=true;
				}
				else 
				{
					document.getElementById("updateEmergency").checked=false;
				}
				//document.getElementById("updateEmergency").value = data.emergency;
				//document.getElementById("updateActive").value = data.active;
				
				
				
		    });
		}
       
        
      </script>
      
      
      
      
            
